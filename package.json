{
  "name": "react-enterprise-boilerplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "tsc && vite build",
    "preview": "npm run build && vite preview --port 3000",
    "test:unit": "vitest --environment jsdom",
    "test:unit:coverage": "vitest run --coverage --environment jsdom",
    "test:e2e": "start-server-and-test preview http-get://127.0.0.1:3000 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http-get://127.0.0.1:3000 'cypress run --e2e'",
    "cypress:open": "cypress open --e2e",
    "cypress:run": "cypress run --e2e",
    "lint": "eslint . --ext .ts,.tsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "stylelint": "npx stylelint \"./**/*.{css,scss,sass}\" --config \"./stylelint.config.js\"",
    "stylelint:fix": "npm run stylelint -- --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "./src/*.{js,jsx,ts,tsx}": "npm run lint",
    "./src/*.{css,scss,sass}": "npm run stylelint",
    "./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": "prettier -w -u"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.17",
    "@chakra-ui/react": "^2.5.0",
    "@chakra-ui/theme": "^2.2.5",
    "@chakra-ui/theme-tools": "^2.0.17",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@reduxjs/toolkit": "^1.9.2",
    "axios": "^1.2.0",
    "clsx": "^1.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "framer-motion": "^9.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.3",
    "react-slick": "^0.29.0",
    "slick-carousel": "^1.8.1",
    "uuidv4": "^6.2.13",
    "vite-plugin-svgr": "^2.4.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@testing-library/cypress": "^8.0.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/react-slick": "^0.23.10",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "cypress": "^11.2.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-vitest-globals": "^1.2.0",
    "husky": "^8.0.2",
    "jsdom": "^20.0.3",
    "prettier": "^2.8.0",
    "sass": "^1.56.1",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^14.15.0",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-scss": "^4.3.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-stylelint": "^3.2.0",
    "vitest": "^0.25.3"
  }
}
