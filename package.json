{
  "name": "steam-dupe",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "tsc && vite build",
    "preview": "npm run build && vite preview --port 3000",
    "test:unit": "vitest --environment jsdom",
    "test:unit:coverage": "vitest run --coverage --environment jsdom",
    "lint": "eslint . --ext .ts,.tsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "./src/*.{js,jsx,ts,tsx}": "npm run lint",
    "./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": "prettier -w -u"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.17",
    "@chakra-ui/react": "^2.5.0",
    "@chakra-ui/theme": "^2.2.5",
    "@chakra-ui/theme-tools": "^2.0.17",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@reduxjs/toolkit": "^1.9.2",
    "axios": "^1.2.0",
    "clsx": "^1.2.1",
    "dompurify": "^3.0.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "framer-motion": "^9.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.3",
    "uuidv4": "^6.2.13",
    "vite-plugin-svgr": "^2.4.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@testing-library/cypress": "^8.0.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/dompurify": "^2.4.0",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/react-slick": "^0.23.10",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-vitest-globals": "^1.2.0",
    "husky": "^8.0.2",
    "jsdom": "^20.0.3",
    "prettier": "^2.8.0",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^0.25.3"
  }
}
